import builtins
from typing import BinaryIO

try:
    from collections.abc import Buffer  # type: ignore[unresolved-import]
except ImportError:
    Buffer = builtins.bytes | builtins.bytearray | memoryview

class Field:
    number: int
    wire_type: int

    def expect(self, wire_type: int) -> Field: ...
    def as_scalar(
        self, scalar: int
    ) -> builtins.int | builtins.bool | builtins.float: ...
    def bytes(self) -> builtins.bytes: ...
    def string(self) -> str: ...
    def message(self) -> Reader: ...
    def skip(self) -> None: ...
    def repeated(
        self, scalar: int
    ) -> list[builtins.int] | list[builtins.float] | list[builtins.bool]: ...

class Reader:
    def __init__(self, stream: BinaryIO) -> None: ...
    def __iter__(self) -> Reader: ...
    def __next__(self) -> Field: ...
    def next(self) -> Field | None: ...
    def remaining(self) -> int | None: ...

class Writer:
    def __init__(self, stream: BinaryIO) -> None: ...
    def write_tag(self, field_number: int, wire_type: int) -> None: ...
    def write_scalar(self, scalar: int, value: object) -> None: ...
    def write_len(self, value: Buffer) -> None: ...
    def write_varint(self, value: int) -> None: ...
    def flush(self) -> None: ...

__all__: list[str]
